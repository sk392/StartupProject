<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sk392.kr.carmony.Fragment.OwnerRegisteredFragment">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="58dp"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="58dp"
            android:layout_marginTop="10dp"
            android:background="@color/white"
            android:orientation="vertical">


            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/iv_owner_page_profile"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/profile" />

            <TextView
                android:id="@+id/tv_owner_page_name"
                style="@style/Blacktwoappleregular18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="156dp"
                android:layout_marginTop="106dp"
                android:text="김상국" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="138dp"
                android:gravity="center_vertical">


                <RatingBar
                    android:id="@+id/rb_owner_page_review"
                    style="@style/customRatingBarMedium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:isIndicator="true"
                    android:rating="5.0"
                    android:stepSize="0.5" />

                <TextView
                    android:id="@+id/tv_owner_pager_review_score"
                    style="@style/Blackapplemedium16"
                    android:layout_width="wrap_content"
                    android:layout_toRightOf="@id/rb_owner_page_review"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="4.1 " />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="151dp"
                android:layout_marginTop="159dp">

                <TextView
                    style="@style/Greyishappleregular12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/res_num" />

                <TextView
                    android:id="@+id/tv_owner_page_res_num"
                    style="@style/Blackappleregular12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="0" />
            </LinearLayout>


            <Button
                android:id="@+id/bt_owner_car_info"
                style="@style/Warmgreyapple15"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="200dp"
                android:textColor="@drawable/button_main_text_type"
                android:background="@drawable/button_main_type"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/car_info" />

            <Button
                android:id="@+id/bt_owner_schedule"
                style="@style/Warmgreyapple15"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="25dp"
                android:layout_marginTop="200dp"
                android:textColor="@drawable/button_main_text_type"
                android:background="@drawable/button_main_type"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/schedule_registering" />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="278.5dp"
                android:background="@color/pale_grey">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/silver" />

                <TextView
                    style="@style/Slateblueappleregular12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="23dp"
                    android:text="@string/income" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="44dp"
                    android:background="@color/silver" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="323.5dp">

                <TextView
                    style="@style/Blacktwoappleregular14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="22.5dp"
                    android:text="@string/all_date" />

                <TextView
                    android:id="@+id/tv_owner_page_total_income"
                    style="@style/Reddishorangeappleregular18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="25dp"
                    android:layout_marginTop="17.5dp"
                    android:text="0원" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="368.5dp">

                <TextView
                    style="@style/Blacktwoappleregular14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="22.5dp"
                    android:text="@string/this_month" />

                <TextView
                    android:id="@+id/tv_owner_page_month_income"
                    style="@style/Reddishorangeappleregular18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="25dp"
                    android:layout_marginTop="17.5dp"
                    android:text="0원" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="423.5dp"
                android:background="@color/pale_grey">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/silver" />

                <TextView
                    style="@style/Slateblueappleregular12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="23dp"
                    android:text="@string/res" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="44dp"
                    android:background="@color/silver" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="468.5dp">

                <TextView
                    style="@style/Blacktwoappleregular14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="22.5dp"
                    android:text="@string/all_date" />

                <TextView
                    android:id="@+id/tv_owner_page_total_res"
                    style="@style/Reddishorangeappleregular18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="25dp"
                    android:layout_marginTop="17.5dp"
                    android:text="0원" />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="513.5dp">

                <TextView
                    style="@style/Blacktwoappleregular14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="22.5dp"
                    android:text="@string/this_month" />

                <TextView
                    android:id="@+id/tv_owner_page_month_res"
                    style="@style/Reddishorangeappleregular18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="25dp"
                    android:layout_marginTop="17.5dp"
                    android:text="0회" />
            </RelativeLayout>
        </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>
</android.support.design.widget.CoordinatorLayout>
